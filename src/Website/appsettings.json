{
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|\\Umbraco.sdf;Flush Interval=1;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Global": {
        "Id": "a6a18915-b6f1-490b-9ab7-d33f7aa09e68"
      }
    }
  },
  "GraphQL": {
    "Server": {
      "EnablePlayground": true,
      "EnableMetrics": true,
      "EnableCors": true,
      "CorsPolicyName": "GraphQLCors",
      "CorsAllowedOrigins": [ "*" ]
    }
  }
}
